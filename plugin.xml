<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<?eclipse version="3.0"?><plugin>

   <!-- This is an example for providing an own category 
      <extension
         point="org.knime.workbench.repository.categories">
      <category
            description="Category containing my nodes"
            icon="icons/default.png"
            level-id="myNodes"
            name="My nodes"
            path="/"/>
   </extension>
   -->2
   
<extension point="org.knime.workbench.repository.nodes">
<node
      category-path="/inflor/read"
      deprecated="false"
      factory-class="main.java.inflor.knime.nodes.fcs.read.ReadFCSSetNodeFactory"/>
<node
       after="main.java.inflor.knime.nodes.fcs.read.ReadFCSSetNodeFactory"
       category-path="/inflor/read"
       deprecated="false"
       factory-class="main.java.inflor.knime.nodes.fcs.read.ReadFCSTableNodeFactory"/>
<node
      after="main.java.inflor.knime.nodes.fcs.read.ReadFCSSetNodeFactory"
      category-path="/inflor/read"
      deprecated="false"
      factory-class="main.java.inflor.knime.nodes.fcs.read.ReadFCSFrameNodeFactory"/>
<node
      category-path="/inflor/comp"
      deprecated="false"
      factory-class="main.java.inflor.knime.nodes.compensation.apply.ApplyCompensationNodeFactory"/>
<node
      after="main.java.inflor.knime.nodes.compensation.apply.ApplyCompensationNodeFactory"
      category-path="/inflor/comp"
      deprecated="false"
      factory-class="main.java.inflor.knime.nodes.compensation.extract.fromfcs.ExtractCommpensationNodeFactory"/>
<node
      after="main.java.inflor.knime.nodes.compensation.apply.ApplyCompensationNodeFactory"
      category-path="/inflor/comp"
      deprecated="false"
      factory-class="main.java.inflor.knime.nodes.compensation.extractfj9mtx.ExtractCompJONodeFactory"/>
<node
      category-path="/inflor/transform"
      deprecated="false"
      factory-class="main.java.inflor.knime.nodes.transform.create.TransformNodeFactory"/>
<node
      category-path="/inflor/util"
      deprecated="false"
      factory-class="main.java.inflor.knime.nodes.downsample.DownsampleNodeFactory"/>
<node
      category-path="/inflor/util"
      deprecated="false"
      factory-class="main.java.inflor.knime.nodes.portToTableCell.ColumnStoreToTableCellNodeFactory"/>
<node
      category-path="/inflor/util"
      deprecated="false"
      factory-class="main.java.inflor.knime.nodes.createGates.CreateGatesNodeFactory"/>
<node
      category-path="/inflor/analyze"
      deprecated="false"
      factory-class="main.java.inflor.knime.nodes.summaryStats.SummaryStatisticsNodeFactory"/>
<node
      category-path="/inflor/analyze"
      deprecated="false"
      factory-class="main.java.inflor.knime.nodes.utility.extractsubset.ExtractTrainingSetNodeFactory"/>
<node
      category-path="/inflor/analyze"
      deprecated="false"
      factory-class="main.java.inflor.knime.nodes.sne.bhtsne.TSNENodeFactory">
</node>
</extension>

<extension point="org.knime.workbench.repository.categories">
		<category icon="META-INF/icons/inflor_knime.png" level-id="inflor" locked="true" name="Inflor" path="/"/>
  <category
        description="Nodes for reading FCS Files (eg. to table, frame or set)."
        icon="META-INF/icons/inflor_knime.png"
        level-id="read"
        locked="true"
        name="Read FCS"
        path="/inflor">
  </category>
  <category
        after="read"
        description="Nodes pertaining to the creation and application of compensation matricies"
        icon="META-INF/icons/inflor_knime.png"
        level-id="comp"
        locked="true"
        name="Compensate"
        path="/inflor">
  </category>
  <category
        after="comp"
        description="Nodes related to creating or altering display transformations (eg. linear, log, or logicle)"
        icon="META-INF/icons/inflor_knime.png"
        level-id="transform"
        locked="true"
        name="Transform"
        path="/inflor">
  </category>
  <category
        after="transform"
        description="Nodes to further ananlysis and characterization of cytometry data (eg. gating, sne, clustering)"
        icon="META-INF/icons/inflor_knime.png"
        level-id="analyze"
        locked="true"
        name="Analyze"
        path="/inflor">
  </category>
  <category after="analyze" icon="META-INF/icons/inflor_knime.png" level-id="util" locked="true" name="Utilities" path="/inflor">
  </category>
</extension>

<extension point="org.knime.core.PortType">
      <portType color="#9B30FF" hidden="false" name="Inflor.FCSFramePort" objectClass="main.java.inflor.knime.ports.fcs.FCSFramePortObject" objectSerializer="main.java.inflor.knime.ports.fcs.FCSFramePortObject$Serializer" specClass="main.java.inflor.knime.ports.fcs.FCSFramePortSpec" specSerializer="main.java.inflor.knime.ports.fcs.FCSFramePortSpec$Serializer"/>
      <portType color="#F300FF" hidden="false" name="Inflor.compMatrix" objectClass="main.java.inflor.knime.ports.compensation.CompMatrixPortObject" objectSerializer="main.java.inflor.knime.ports.compensation.CompMatrixPortObject$Serializer" specClass="main.java.inflor.knime.ports.compensation.CompMatrixPortSpec" specSerializer="main.java.inflor.knime.ports.compensation.CompMatrixPortSpec$Serializer">
      </portType>
</extension>
<extension point="org.knime.core.DataType">
   <DataType cellClass="main.java.inflor.knime.data.type.cell.fcs.FCSFrameFileStoreDataCell" factoryClass="main.java.inflor.knime.data.type.cell.fcs.FCSFrameCellFactory">
      <serializer cellClass="main.java.inflor.knime.data.type.cell.fcs.FCSFrameFileStoreDataCell" serializerClass="main.java.inflor.knime.data.type.cell.fcs.FCSFrameFileStoreDataCell$Serializer">
      </serializer>
   </DataType>
</extension>
<extension point="org.knime.core.DataValueRenderer">
   <dataValue valueClass="main.java.inflor.knime.data.type.cell.fcs.FCSFrameDataValue">
      <renderer rendererFactoryClass="main.java.inflor.knime.data.type.cell.fcs.FCSFrameCellRenererFactory" suggestAsDefault="true">
      </renderer>
   </dataValue>
</extension>
   
</plugin>
